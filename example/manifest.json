{
    "name": "DailyBugle",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "pub/sub for chrome-extensions",
    "background": {
        "scripts": [
            "PrintingPress.js",
            "requireLibs.js",
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "icons": {
            "128": "db.jpg"
        },
        "default_title": "DailyBugle"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "activeTab"
    ]
}
